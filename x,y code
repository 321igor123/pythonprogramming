def f(x,y):
    if x > 0 and y > 0:
        return "I"
    elif x < 0 and y > 0:
        return "II"
    elif x < 0 and y < 0:
        return "III"
    elif x > 0 and y < 0:
        return "IIII"
    elif x == 0 and y == 0:
        return "origin of coordinates"


        
x = int(input())
y = int(input())
print(f(x,y))
